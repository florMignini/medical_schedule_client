{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["esnext", "dom"],
    "types": ["cypress", "@testing-library/cypress"],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}

